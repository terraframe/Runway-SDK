<Runway xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <create>
    <enumerationMaster label="Enumeration Class Test" name="test.xmlclasses.EnumClassTest">
      <attributes>
        <char label="State Name" size="2" name="testChar" />
      </attributes>
    </enumerationMaster>

    <object type="test.xmlclasses.EnumClassTest" key="test.xmlclasses.EnumClassTest.CO">
      <attribute name="testChar" value="CO" />
      <attribute name="enumName" value="CO" />
      <attribute name="displayLabel" value="Colorado" />
    </object>

    <object type="test.xmlclasses.EnumClassTest" key="test.xmlclasses.EnumClassTest.CA">
      <attribute name="testChar" value="CA" />
      <attribute name="enumName" value="CA" />
      <attribute name="displayLabel" value="California" />
    </object>

    <object type="test.xmlclasses.EnumClassTest" key="test.xmlclasses.EnumClassTest.NY">
      <attribute name="testChar" value="NY" />
      <attribute name="enumName" value="NY" />
      <attribute name="displayLabel" value="New York" />
    </object>

    <mdEnumeration type="test.xmlclasses.EnumClassTest" label="Enumeration Filter Test" name="test.xmlclasses.Filter1" description="Filter Set Test" removable="false">
      <addEnumItem name="CO" />
      <addEnumItem name="CA" />
    </mdEnumeration>

    <mdEnumeration type="test.xmlclasses.EnumClassTest" label="Enumeration Include All" name="test.xmlclasses.Filter2">
      <includeAll />
    </mdEnumeration>
  </create>
</Runway>
